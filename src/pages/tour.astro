---
import Layout from '../layouts/Layout.astro';
import '../styles/components.css';
import TourCard from '../components/TourCard.astro';
import tours from '../data/tour.json';
---
<Layout>
    <section class="tour">
        <div class="tour-header">
            <h1>Live</h1>
        </div>
        <div class="tour-list">
            {tours.map((tour: any) => (
                <TourCard tour={tour} />
            ))}
        </div>
    </section>
</Layout>