{
    "extends": "astro/tsconfigs/strict",
    "compilerOptions": {
        "resolveJsonModule": true,
        "esModuleInterop": true
    },
    "include": [
        ".astro/types.d.ts",
        "src/**/*.astro",
        "src/**/*.ts",
        "src/**/*.js",
        "src/**/*.json",
        "astro.config.mjs",
        "env.d.ts"
    ],
    "exclude": [
        "node_modules",
        "dist",
        "public"
    ]
}
